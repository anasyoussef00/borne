{
  "name": "borne",
  "private": true,
  "version": "0.2.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "devtools": "vue-devtools",
    "tauri:dev": "run-p devtools \"tauri dev\"",
    "tauri:dev-host": "run-p devtools \"tauri dev --host\"",
    "lint": "eslint --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mt --ignore-path .gitignore",
    "lint:fix": "eslint --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mt --ignore-path .gitignore --fix",
    "format": "prettier . --write",
    "test:e2e": "playwright test",
    "test:unit": "vitest"
  },
  "dependencies": {
    "@tauri-apps/api": "^1.4.0",
    "pinia": "^2.1.4",
    "sweetalert2": "^11.7.12",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-brands-svg-icons": "^6.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/vue-fontawesome": "^3.0.3",
    "@playwright/test": "^1.35.1",
    "@tauri-apps/cli": "^1.4.0",
    "@types/node": "^18.7.10",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/devtools": "^6.5.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.4.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.43.0",
    "eslint-plugin-vue": "^9.15.1",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.24",
    "prettier": "2.8.8",
    "tailwindcss": "^3.3.2",
    "typescript": "^4.9.5",
    "vite": "^4.2.1",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^0.32.2",
    "vue-tsc": "^1.0.11"
  }
}
